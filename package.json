{
  "name": "xp-storybook",
  "version": "0.0.1",
  "description": "Creates a preview for storybook server",
  "private": true,
  "scripts": {
    "build:server": "webpack --config webpack.server.config.js --color",
    "build": "run-p -c build:*",
    "dist:server": "cross-env NODE_ENV=production webpack --config webpack.server.config.js --color --hide-modules",
    "dist": "run-p -c dist:*"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "@enonic-types/lib-export": "^7.11.3",
    "@item-enonic-types/global": "^7.11.3",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "babel-loader": "^9.1.2",
    "browserslist-config-enonic": "^1.0.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.2.0",
    "glob": "^8.1.0",
    "prettier": "^2.8.3",
    "ramda": "^0.28.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "browserslist": [
    "extends browserslist-config-enonic"
  ],
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 7.10.0"
  }
}
